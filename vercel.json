{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "env": {
    "VITE_API_URL": "https://servigo-04kk.onrender.com/api",
    "VITE_APP_NAME": "ServiGo",
    "VITE_ENV": "production"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  }
}
